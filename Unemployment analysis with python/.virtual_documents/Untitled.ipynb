import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns


data = pd.read_csv("Unemployment in India.csv")


data


data.info()


data.isnull().sum()


data.describe()


data.columns


plt.figure(figsize=(10,5))
sns.lineplot(x=' Date', y=' Estimated Unemployment Rate (%)', data=data, color='orange')
plt.title("ğŸ“‰ Unemployment Rate Over Time in India", fontsize=14)
plt.xlabel("Date")
plt.ylabel("Unemployment Rate (%)")
plt.grid(True)
plt.show()



plt.figure(figsize=(10,6))
sns.barplot(x='Region', y=' Estimated Unemployment Rate (%)', data=data, palette='viridis')
plt.title("ğŸ“Š Region-wise Unemployment Rate", fontsize=14)
plt.xticks(rotation=90)
plt.show()


plt.figure(figsize=(8,5))
sns.heatmap(
    data[[' Estimated Unemployment Rate (%)', 
          ' Estimated Employed', 
          ' Estimated Labour Participation Rate (%)']].corr(),
    annot=True, cmap='coolwarm'
)
plt.title("ğŸ” Correlation Heatmap", fontsize=13)
plt.show()

